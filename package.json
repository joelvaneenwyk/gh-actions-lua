{
  "name": "install-lua",
  "version": "1.0.0",
  "description": "Build and install Lua",
  "main": "main.js",
  "scripts": {
    "clean": "git clean -xfd --no-interactive && yarn install",
    "build": "ncc build main.js --target es2015 --license license.txt --no-cache --source-map --stats-out dist/stats.json --minify -o dist",
    "dev": "ncc build main.js --target es2015 --license license.txt --source-map --stats-out dist/stats.json -o dist",
    "test": "yarn build && node dist/index.js --luaVersion \"5.1.5\"",
    "start:debug": "node --inspect-brk=5858 ./main.js --luaVersion \"5.1.5\""
  },
  "author": "leaf corcoran <leafot@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^3.2.4",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.1"
  }
}
